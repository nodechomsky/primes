var rule= 110;
var ruleStr = rule.toString(2);
var ruleAr[] = ruleStr.split("");

for (row = 0; row < 8; row++) 
 {
  for (col = 0; col < 8; col++) 
  {
   console.log(ruleAr[col]);
  }
 }


function 2dA( row , col )
 {
  for (row = 0; row < 8; row++) 
  {
   for (col = 0; col < 8; col++) 
   {
    console.log(ruleAr[col]);
   }
  }
 }
